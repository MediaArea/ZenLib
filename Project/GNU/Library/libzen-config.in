#!/bin/sh

if test "$1" = "CXXFLAGS"; then
echo @ZenLib_CXXFLAGS@
fi
if test "$1" = "LIBS"; then
echo @ZenLib_LIBS@
fi
if test "$1" = "LIBS_Static"; then
echo @ZenLib_LIBS_Static@
fi
if test "$1" = "Unicode"; then
echo @ZenLib_Unicode@
fi
if test "$1" = "WstringMissing"; then
echo @ZenLib_wstring_missing@
fi
if test "$1" = "Exists"; then
echo yes
fi
